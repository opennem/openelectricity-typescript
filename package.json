{
  "name": "@openelectricity/client",
  "version": "0.0.4",
  "description": "OpenElectricity API Typescript/Javascript client",
  "main": "./dist/node/index.js",
  "module": "./dist/browser/index.js",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.js",
        "types": "./dist/node/index.d.ts"
      },
      "browser": {
        "import": "./dist/browser/index.js",
        "types": "./dist/browser/index.d.ts"
      },
      "default": "./dist/browser/index.js"
    },
    "datatable": {
      "node": "./dist/node/datatable.js",
      "browser": "./dist/browser/datatable.js",
      "types": "./dist/node/datatable.d.ts"
    }
  },
  "browser": "./dist/browser/index.js",
  "type": "module",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./bin/build",
    "test": "bun test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "publish": "./bin/npm-publish"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bun": "latest",
    "eslint": "^9.20.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jq": "^1.7.2",
    "prettier": "^3.5.0",
    "rollup": "^4.9.6"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "volta": {
    "node": "22.14.0",
    "yarn": "1.22.22"
  }
}
