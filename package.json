{
  "name": "@openelectricity/client",
  "version": "0.0.10",
  "description": "OpenElectricity API Typescript/Javascript client",
  "main": "./dist/node/index.js",
  "module": "./dist/browser/index.js",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "node": {
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.js"
      },
      "import": "./dist/browser/index.js",
      "require": "./dist/node/index.js"
    },
    "./datatable": {
      "types": "./dist/node/datatable.d.ts",
      "node": {
        "import": "./dist/node/datatable.js",
        "require": "./dist/node/datatable.js"
      },
      "import": "./dist/browser/datatable.js",
      "require": "./dist/node/datatable.js"
    }
  },
  "browser": "./dist/browser/index.js",
  "type": "module",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./bin/build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "publish": "./bin/npm-publish"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bun": "latest",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jq": "^1.7.2",
    "prettier": "^3.5.0",
    "rollup": "^4.9.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "volta": {
    "node": "22.14.0"
  }
}
